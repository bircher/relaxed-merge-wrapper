{
    "name": "bircher/relaxed-merge-wrapper",
    "description": "Wrap the merge implementation of relaxedws",
    "require": {
        "bircher/php-merge": "dev-master",
        "relaxedws/merge": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "repositories": [
        {
            "type": "git",
            "url":  "https://github.com/relaxedws/merge.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "RelaxedMerge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RelaxedMerge\\Test\\": "tests/",
            "PhpMerge\\Test\\": "vendor/bircher/php-merge/test/PhpMerge/Test"
        }
    }
}
